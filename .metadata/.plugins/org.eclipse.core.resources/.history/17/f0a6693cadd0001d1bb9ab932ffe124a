#include <stdio.h>
#include <stdint.h>

int main(void)
{
	uint8_t data[10] = {0xFF,0xFF,0xFF,0xFF,0xFF,0xFF,0xFF,0xFF,0xFF,0xFF};

	// printing the elements using a loop
	for(uint32_t i = 0; i<10 ; i++)
	{
		printf("%x\t",data[i]);
	}


	getchar();
	return 0;
}

// Video 190
#include <stdio.h>
// using function like macro
#define PI 3.1415f
#define AREA_OF_CIRCLE(r) PI*r*r


int main(void)
{

	float areaCircle;
	areaCircle =  AREA_OF_CIRCLE(1+1);// using the macro for area
	printf("Area = %f\n",areaCircle);

	return 0;
}
